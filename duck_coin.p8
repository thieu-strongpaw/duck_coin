pico-8 cartridge // http://www.pico-8.com
version 43
__lua__
function _init()
	col = 7
	iplr()
	init_coins(2)
	score = 0

end

x = 1 y = 1

function _update()
	if (btnp(5)) col = 8 + rnd(8)
	uplr()
	update_coin()
end

function _draw()
	cls(1)
	circfill(x,y,29,col)
	map()
	camera(plr.x-64,plr.y-64)
	dplr()
	display_coins()
	draw_hud()
end

-- player init
function iplr()
	plr = {
		x = 64,
		y = 64
	}
end

-- coin init
function init_coins(n)
	coins = {}
	for i=1,n do
		add(coins, {x=rnd(32)+32, y=rnd(32)+18, show=1})
		end
end

-- player update
function uplr()
	move(plr)
end

-- coin update
function update_coin()
	for c in all(coins) do 
		check_coin(c)
	end
end

-- player draw
function dplr()
	spr(32, plr.x, plr.y)
end

-- coin draw
function display_coins()
	for c in all(coins) do
		if c.show == 1 then
			spr(16, c.x, c.y)
	end
	end
end

-- movement + wall collision
function move(o)
	local lx=o.x
	local ly=o.y
	
	if (btn(0)) o.x-=1
	if (btn(1)) o.x+=1
	if (btn(2)) o.y-=1
	if (btn(3)) o.y+=1
	
	-- if collide returns true, move back
	if collide(o) then
		o.x=lx
		o.y=ly
	end
end

-- check player/coin collision
function check_coin(c)
	if plr.x < c.x+8 and
	   plr.x+8 > c.x and
	   plr.y < c.y+8 and
	   plr.y+8 > c.y and
	   c.show == 1 then
	   
		c.show = 0
		score += 1
	end
end

-- tilemap collision check
function collide(o)
	local x1=o.x/8
	local y1=o.y/8
	local x2=(o.x+7)/8
	local y2=(o.y+7)/8
	
	local a=fget(mget(x1,y1),0)
	local b=fget(mget(x1,y2),0)
	local c=fget(mget(x2,y1),0)
	local d=fget(mget(x2,y2),0)

	if a or b or c or d then 
		return true
	else
		return false
	end
end

function draw_hud()
	plr_x = plr.x
	plr_y = plr.y 
	rectfill(plr_x-60,plr_y-60,plr_x-48,plr_y-45,12)
	print(score,plr_x-55,plr_y-55,1)
end
